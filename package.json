    "@jest/globals": "^29.3.1",
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "chai": "^4.3.7",
    "eslint": "^8.54.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "ts-jest": "^29.0.5",
